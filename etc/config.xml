<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mfspeedoptimizations>
            <general>
                <enabled>0</enabled>
                <mfmodule>SpeedOptimization</mfmodule>
                <mftype>1</mftype>
            </general>
            <css>
                <move_print_css_to_bottom>0</move_print_css_to_bottom>
            </css>
            <database>
                <clean_database_enabled>0</clean_database_enabled>
            </database>

            <session>
                <disable_session_for_robots>1</disable_session_for_robots>
                <robots_user_agents>bot
crawl
slurp
spider
mediapartners
ia_archiver
facebookexternalhit
pingdom
monitor
composer</robots_user_agents>
            </session>
            <gallery>
                <enabled>0</enabled>
                <thumbs_direction>horizontal</thumbs_direction>
            </gallery>
        </mfspeedoptimizations>
        <mflazyzoad>
            <general>
                <mfmodule>SpeedOptimization</mfmodule>
            </general>
        </mflazyzoad>
        <mfrocketjavascript>
            <general>
                <mfmodule>SpeedOptimization</mfmodule>
            </general>
        </mfrocketjavascript>
    </default>
</config>
